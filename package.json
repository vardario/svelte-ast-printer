{
  "name": "@vardario/svelte-ast-printer",
  "version": "0.0.0-development",
  "description": "This library converts a svelte ast to a string.",
  "license": "MIT",
  "author": "Sahin Vardar",
  "repository": {
    "type": "git",
    "url": "git@github.com:vardario/svelte-ast-printer.git"
  },
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc --build",
    "format": "prettier --plugin-search-dir . --write . && prettier-package-json --write",
    "preinstall": "npx only-allow pnpm",
    "semantic-release": "semantic-release",
    "test": "vitest run",
    "watch": "tsc --watch",
    "repo-clean": "git clean -xdf -e .env"
  },
  "dependencies": {
    "@vardario/astring-ts-generator": "^1.1.1",
    "astring": "^1.9.0",
    "commander": "^10.0.1",
    "estree-walker": "^3.0.3",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "svelte": "5.x"
  },
  "devDependencies": {
    "@types/estree": "^1.0.6",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "lodash": "^4.17.21",
    "prettier": "^3.1.0",
    "prettier-package-json": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "semantic-release": "^22.0.8",
    "svelte": "^5.1.3",
    "typescript": "^5.0.4",
    "vitest": "^2.1.3"
  },
  "keywords": [],
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "package.json": "prettier-package-json --write"
  },
  "packageManager": "pnpm@9.1.0"
}
